    max-width: 90%;
    vertical-align: middle;
    text-align: center;
    font-size: 18px;
    transform: scale(0.9);
    -ms-transform: scale(0.9);
    -webkit-transform: scale(0.9);
    opacity: 0;
    visibility: hidden;
    transition: all ease-in-out .3s;
}

.open-search-form .searchform-fly {
