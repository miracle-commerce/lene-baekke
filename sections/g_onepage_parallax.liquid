{%- case section.settings.sidetoshow -%}
    {% when '3' %}
      	{%- assign slider_class_width = 'xlg-grid-3-items lg-grid-3-items md-grid-3-items' -%}
    {% when '4' %}
    	{%- assign slider_class_width = 'xlg-grid-4-items lg-grid-4-items md-grid-4-items' -%}
{%- endcase -%}

{%- case section.settings.nav_style -%}
	{% when '1' %}
		{%- assign nav_class = 'g-nav-onepage-1' -%}
	{% when '2' %}
		{%- assign nav_class = 'g-nav-onepage-2' -%}
	{% when '3' %}
		{%- assign nav_class = 'g-nav-onepage-3' -%}
{%- endcase -%}

{%- case section.settings.catalog_position -%}
    {% when '1' %}
      	{%- assign catalog_class = 'catalog_position-1' -%}
    {% when '2' %}
    	{%- assign catalog_class = 'catalog_position-2' -%}
{%- endcase -%}

{%- case section.settings.social_position -%}
    {% when '1' %}
      	{%- assign social_class = 'social_position-1' -%}
    {% when '2' %}
    	{%- assign social_class = 'social_position-2' -%}
{%- endcase -%}

<div class="g-fullpage">
  {% if section.settings.use_nav %}
  <div id="onepage-actions" class="{{ nav_class }}">
    <a id="moveSectionUp" href="#"><i class="icon dl-icon-up"></i></a>
    <a id="moveSectionDown" href="#"><i class="icon dl-icon-down"></i></a>
  </div>
  {% endif %}
  {% if section.settings.use_dot %}
  <style>
    #fp-nav.fp-right {display: block;}
    #fp-nav ul li a span, .fp-slidesNav ul li a span {background: {{ section.settings.dot_color }} !important;}
  </style>
  {% endif %}
  {% if section.settings.catalog_text != blank %}
  <div class="catalog-onepage {{ catalog_class }}"><a href="{{ section.settings.catalog_link }}" {% if section.settings.catalog_text_color != blank %}style="color: {{ section.settings.catalog_text_color }};"{% endif %}>{{ section.settings.catalog_text }}</a></div>
  {% endif %}
  {% if section.settings.copyright_text != blank %}
  <div class="copyright-onepage {{ social_class }}" {% if section.settings.copyright_text_color != blank %}style="color: {{ section.settings.copyright_text_color }};"{% endif %}>{{ section.settings.copyright_text }}</div>
  {% endif %}
  {% if section.settings.use_phone_number_slider %}
  <div class="phone-number-slider" {% if section.settings.color_phone_number != blank %}style="color: {{ section.settings.color_phone_number }};"{% endif %}>
    <div class="icon" style="{% if section.settings.bg_color_phone_number_icon != blank %}background-color: {{ section.settings.bg_color_phone_number_icon }};{% endif %} {% if section.settings.color_phone_number_icon != blank %}color: {{ section.settings.color_phone_number_icon }};{% endif %}"><i class="fa fa-phone"></i></div>{{ section.settings.text_phone_number_slider }}
  </div>
  {% endif %}
  <div id="fullpage">
    {% for block in section.blocks %}
    {% case block.type %}
    {% when 'section1' %}
    <div class="section">
      {% if block.settings.image != blank %}
      <div class="cover-background-parallax" style="background-image: url({{ block.settings.image | img_url: '1920x', format: 'pjpg' | split:'?' | first }});"></div>
      {% else %}
      {% capture current %}{% cycle 1, 2 %}{% endcapture %}
      {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
      {% endif %} 
      <div class="onepage-content-section-1">
        {% if block.settings.title != blank %}
        <h2 {% if block.settings.title_color != blank %}style="color: {{ block.settings.title_color }};"{% endif %}>{{ block.settings.title }}</h2>
        {% endif %}
        <div class="text-and-button">
        {% if block.settings.text != blank %}
        <div class="text" {% if block.settings.text_color != blank %}style="color: {{ block.settings.text_color }};"{% endif %}>{{ block.settings.text }}</div>
        {% endif %}
        {% if block.settings.button_name != blank %}
        <a href="{{ block.settings.button_link }}" {% if block.settings.button_color != blank %}style="color: {{ block.settings.button_color }};"{% endif %}>{{ block.settings.button_name }}</a>
        {% endif %}
        </div>
      </div>
      {% if block.settings.text_info != blank %}
      	<div class="text-info-slider" {% if block.settings.text_info_color != blank %}style="color: {{ block.settings.text_info_color }};"{% endif %}>{{ block.settings.text_info }}</div>
      {% endif %}
