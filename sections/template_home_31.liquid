{% for block in section.blocks %}
{%- case block.settings.css_animations -%}
    {% when '1' %}
      	{%- assign css_animations_class = '' -%}
    {% when '2' %}
    	{%- assign css_animations_class = 'wow fadeIn' -%}
    {% when '3' %}
    	{%- assign css_animations_class = 'wow fadeInDown' -%}
    {% when '4' %}
    	{%- assign css_animations_class = 'wow fadeInDownBig' -%}
    {% when '5' %}
    	{%- assign css_animations_class = 'wow fadeInLeft' -%}
    {% when '6' %}
    	{%- assign css_animations_class = 'wow fadeInLeftBig' -%}
    {% when '7' %}
    	{%- assign css_animations_class = 'wow fadeInRight' -%}
    {% when '8' %}
    	{%- assign css_animations_class = 'wow fadeInRightBig' -%}
    {% when '9' %}
    	{%- assign css_animations_class = 'wow fadeInUp' -%}
    {% when '10' %}
    	{%- assign css_animations_class = 'wow fadeInUpBig' -%}
{%- endcase -%}

{%- case block.settings.content_align -%}
  {% when '1' %}
  	{%- assign content_align_class = 'content_align-left' -%}
  {% when '2' %}
  	{%- assign content_align_class = 'content_align-center' -%}
  {% when '3' %}
  	{%- assign content_align_class = 'content_align-right' -%}
{%- endcase -%}
  {% case block.type %}
  {% when 'section1' %}
	<div class="section-t-p lazy" {% if block.settings.image != blank %}data-src="{{ block.settings.image | img_url: '1920x', format: 'pjpg' | split:'?' | first }}" {% if block.settings.padding != blank %}style="padding: {{ block.settings.padding }};"{% endif %}{% else %}style="background-color: {{ block.settings.bg_color }};  {% if block.settings.padding != blank %}padding: {{ block.settings.padding }};{% endif %}"{% endif %}>
      {% if block.settings.image_before != blank %}
      <div class="before-section-t-p-1 lazy" data-src="{{ block.settings.image_before | img_url: '590x500' | split:'?' | first }}"></div>
      {% endif %}
      <div class="container">
        <div class="content-t-h-31 {{ content_align_class }} {{ css_animations_class }}">
          {% if block.settings.heading != blank %}
          <h2 {% if block.settings.heading_color != blank %}style="color: {{ block.settings.heading_color }};"{% endif %}>{{ block.settings.heading }}</h2>
          {% endif %}
          {% if block.settings.text != blank %}
          <p {% if block.settings.text_color != blank %}style="color: {{ block.settings.text_color }};"{% endif %}>{{ block.settings.text }}</p>
          {% endif %}
          {% if block.settings.use_content_html %}
          <ul class="content-hmtl-s" {% if block.settings.content_html_color != blank %}style="color: {{ block.settings.content_html_color }};"{% endif %}>
            {{ block.settings.content_html }}
          </ul>
          {% endif %}
        </div>
      </div>
      {% if block.settings.show_video_popup %}
      <div class="link-video-section">
        <a href="{{ block.settings.link_video_popup }}" class="video_popup" style="{% if block.settings.bg_icon != blank %}background-color: {{ block.settings.bg_icon }};{% endif %} {% if block.settings.top_position != blank %}top: {{ block.settings.top_position }};{% endif %} {% if block.settings.right_position != blank %}right: {{ block.settings.right_position }};{% endif %}"><span class="icon"><i class="fa fa-play"></i></span></a>
      </div>
      {% endif %}
	</div>
  {% when 'section2' %}
    <div class="section-t-p lazy" {% if block.settings.image != blank %}data-src="{{ block.settings.image | img_url: '1920x', format: 'pjpg' | split:'?' | first }}" {% if block.settings.padding != blank %}style="padding: {{ block.settings.padding }};"{% endif %}{% else %}style="background-color: {{ block.settings.bg_color }};  {% if block.settings.padding != blank %}padding: {{ block.settings.padding }};{% endif %}"{% endif %}>
      <div class="container">
        <div class="row">
          <div class="col-md-5 col-sm-6 col-xs-12 text-right {{ css_animations_class }}">
            {% if block.settings.heading != blank %}
            <h2 {% if block.settings.heading_color != blank %}style="color: {{ block.settings.heading_color }};"{% endif %}>{{ block.settings.heading }}</h2>
            {% endif %}
            {% if block.settings.text != blank %}
            <p {% if block.settings.text_color != blank %}style="color: {{ block.settings.text_color }};"{% endif %}>{{ block.settings.text }}</p>
            {% endif %}
          </div>
          <div class="col-md-6 col-md-offset-1 col-sm-6 col-xs-12 {{ css_animations_class }}">
            {% if block.settings.price != blank %}
            <div class="price" {% if block.settings.price_color != blank %}style="color: {{ block.settings.price_color }};"{% endif %}>{{ block.settings.price }}</div>
            {% endif %}
            {% if block.settings.button_name != blank %}
            <a href="{{ block.settings.button_link }}" class="btn-section-t-p" style="{% if block.settings.button_color != blank %}color: {{ block.settings.button_color }};{% endif %}{% if block.settings.button_bg_color != blank %}background-color: {{ block.settings.button_bg_color }};{% endif %}">{{ block.settings.button_name }}</a>
            {% endif %}
          </div>
        </div>
      </div>
    </div>
  {% when 'section3' %}
  <div class="section-t-p lazy" {% if block.settings.image != blank %}data-src="{{ block.settings.image | img_url: '1920x', format: 'pjpg' | split:'?' | first }}" {% if block.settings.padding != blank %}style="padding: {{ block.settings.padding }};"{% endif %}{% else %}style="background-color: {{ block.settings.bg_color }};  {% if block.settings.padding != blank %}padding: {{ block.settings.padding }};{% endif %}"{% endif %}>
    <div class="container">
      <div class="row">
        <div class="col-md-7 col-sm-6 col-xs-12"></div>
        <div class="col-md-5 col-sm-6 col-xs-12 {{ css_animations_class }}">
          <div style="display: none;">{% include 'dlicon-arrow-left2' %}{% include 'dlicon-arrow-right2' %}</div>
          {% if block.settings.heading != blank %}
          <h2 {% if block.settings.heading_color != blank %}style="color: {{ block.settings.heading_color }};"{% endif %}>{{ block.settings.heading }}</h2>
          {% endif %}
          <div class="g-stick-testimonial g-slick-slider" data-slick='{"infinite":{% if block.settings.use_loop %}true{% else %}false{% endif %},"dots":{% if block.settings.use_dots %}true{% else %}false{% endif %},"slidesToShow":1,"slidesToScroll":1,"autoplay":{% if block.settings.use_autoplay %}true{% else %}false{% endif %},"arrows":{% if block.settings.use_arrows %}true{% else %}false{% endif %},"speed":"1000","autoplaySpeed":3000,"centerMode":false,"variableWidth":false,"vertical":false,"nextArrow":"<button type=\"button\" style=\"font-size:24px;width:24px;height:24px;line-height:24px;\" class=\"slick-next default\"><svg><use xlink:href=\"#dlicon-arrow-right2\"><\/use><\/svg><\/button>","prevArrow":"<button type=\"button\" style=\"font-size:24px;width:24px;height:24px;line-height:24px;\" class=\"slick-prev default\"><svg><use xlink:href=\"#dlicon-arrow-left2\"><\/use><\/svg><\/button>","responsive":[{"breakpoint":1824,"settings":{"slidesToShow":1,"slidesToScroll":1}},{"breakpoint":1200,"settings":{"slidesToShow":1,"slidesToScroll":1}},{"breakpoint":992,"settings":{"slidesToShow":1,"slidesToScroll":1}},{"breakpoint":768,"settings":{"slidesToShow":1,"slidesToScroll":1}},{"breakpoint":480,"settings":{"slidesToShow":1,"slidesToScroll":1}}]}'>
            {% if block.settings.use_client_say_1 %}
            <div class="testimonial_item">
              {% if block.settings.quote_1 != blank %}
              <div class="testimonial_item--info" {% if block.settings.quote_color != blank %}style="color: {{ block.settings.quote_color }};"{% endif %}>
                {{ block.settings.quote_1 }}
              </div>
              {% endif %}
              {% if block.settings.author_1 != blank %}
              <div class="testimonial_item--bottom" {% if block.settings.author_color != blank %}style="color: {{ block.settings.author_color }};"{% endif %}>
                {{ block.settings.author_1 }}
              </div>
              {% endif %}
            </div>
            {% endif %}
            {% if block.settings.use_client_say_2 %}
            <div class="testimonial_item">
              {% if block.settings.quote_2 != blank %}
              <div class="testimonial_item--info" {% if block.settings.quote_color != blank %}style="color: {{ block.settings.quote_color }};"{% endif %}>
                {{ block.settings.quote_2 }}
              </div>
              {% endif %}
              {% if block.settings.author_2 != blank %}
              <div class="testimonial_item--bottom" {% if block.settings.author_color != blank %}style="color: {{ block.settings.author_color }};"{% endif %}>
                {{ block.settings.author_2 }}
              </div>
              {% endif %}
            </div>
            {% endif %}
            {% if block.settings.use_client_say_3 %}
            <div class="testimonial_item">
              {% if block.settings.quote_3 != blank %}
              <div class="testimonial_item--info" {% if block.settings.quote_color != blank %}style="color: {{ block.settings.quote_color }};"{% endif %}>
                {{ block.settings.quote_3 }}
              </div>
              {% endif %}
              {% if block.settings.author_3 != blank %}
              <div class="testimonial_item--bottom" {% if block.settings.author_color != blank %}style="color: {{ block.settings.author_color }};"{% endif %}>
                {{ block.settings.author_3 }}
              </div>
              {% endif %}
            </div>
            {% endif %}
          </div>
        </div>       
      </div>
    </div>
  </div>
  {% endcase %}
{% endfor %}
